FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volScalarField;
    object          csi;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inletfuel           
    {
		type fixedValue;
        value           uniform 1;
    }
    inletair           
    {
		type fixedValue;
        value           uniform 0;
    }

        outlet          
    {
        type            zeroGradient;
    }

    leftside       
    {
        type            zeroGradient;
    }

    burnerwall       
    {
        type            zeroGradient;
    }

    front    
    {
        type            wedge;
    }

    back    
    {
        type            wedge;
    }
    axis
    {
        type            empty;
    }


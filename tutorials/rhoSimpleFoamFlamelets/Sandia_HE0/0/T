FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [ 0 0 0 1 0 0 0 ];

internalField   uniform 294;

boundaryField
{
    inletfuel           
    {
        type  		fixedValue;
 	value 		uniform 295;
    }

    inletair           
    {
        type  		fixedValue;
 	value 		uniform 294;
    }

    outlet          
    {
        type  		zeroGradient;
    }

    leftside       
    {
        type  		zeroGradient;
    }

    burnerwall
    {
        type  		zeroGradient;
    }

    front
    {
        type            wedge;
    }

    back
    {
        type            wedge;
    }

    axis
    {
        type            empty;
    }
}


